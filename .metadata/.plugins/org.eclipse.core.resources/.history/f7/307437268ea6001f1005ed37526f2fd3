package numberprograms;

import java.util.Scanner;

public class Tables {
	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		System.out.println("Enter a number!. which table u wanna print");
		int num = s.nextInt();

		for (int i = 1; i <= num; i++) {
			for (int j = i; j <= 10; j++) {
				System.out.println(i + "x" + j + "=" + (i * j));
			}
		}
	}

}
